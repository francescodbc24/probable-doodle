{
  "name": "react-radio-ts",
  "version": "0.0.12",
  "description": "Simple react radio",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/francescodbc24/react-radio-ts.git"
  },
  "scripts": {
