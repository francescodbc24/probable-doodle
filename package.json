    "jest": "^29.4.3",
    "jest-canvas-mock": "^2.4.0",
    "jest-environment-jsdom": "^29.4.3",
    "parcel": "^2.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "react-jss": "^10.10.0",
